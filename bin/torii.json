{
  "version": "1.0.1",
  "xlsx_file": "tatebayashi_stones.xlsx",
  "geojson_file": "tatebayashi_stones.geojson",
  "tables": {
    "pois": {
      "attrs": [
        ["fid", "ID", "n"],
        ["type", "種別", "s"],
        ["title", "名称", "s"],
        ["era", "和暦", "s"],
        ["year", "年", "s"],
        ["prefecture", "都道府県", "s"],
        ["city", "郡市町村", "s"],
        ["area", "区地域", "s"],
        ["oaza", "大字", "s"],
        ["koaza", "小字", "s"],
        ["detail_place", "詳細場所", "s"],
        ["reference_memo", "参照本情報", "s"],
        ["folklore", "言い伝え", "s"],
        ["history", "歴史", "s"],
        ["survey_memo", "調査情報", "s"],
        ["surveyed", "調査日", "s"],
        ["confirmed", "現況確認済み", "b"],
        ["primary_image", "優先画像ID", "n"],
        ["height", "総高", "n"],
        ["statue_height", "像高", "n"],
        ["width", "幅", "n"],
        ["depth", "厚さ", "n"],
        ["shape", "形状", "s"],
        ["material", "材質", "s"],
        ["inscription", "刻銘", "s"],
        ["color", "色", "s"],
        ["contradiction", "データの矛盾", "s"],
        ["need_action", "要対応", "s"],
        ["status", "状況", "s"],
        ["longitude", "経度", "n"],
        ["latitude", "緯度", "n"]
      ],
      "merge": {
        "images": {
          "link": {
            "fid": "poi"
          },
          "multiple": true
        },
        "books": {
          "target": "refs",
          "link": {
            "fid": "poi"
          },
          "multiple": true
        }
      },
      "image_sort": {
        "priority_key": "primary_image",
        "priority_prefix": "PRIM."
      }
    },
    "images": {
      "thumbnails": [
        ["mid_thumbs", 800],
        ["small_thumbs", 200]
      ],
      "defaults": {
        "poi": "poi.fid",
        "shooting_date": "info.shooting_date",
        "shooter": "info.shooter",
        "description": "poi.title",
      },
      "attrs": [
        ["fid", "ID", "n"],
        ["poi", "地物ID", "n"],
        ["description", "説明", "s"],
        ["path", "画像パス", "s"],
        ["shooting_date", "撮影日", "s"],
        ["shooter", "撮影者", "s"],
        ["mid_thumbs", "中サイズサムネイル", "s"],
        ["small_thumbs", "小サイズサムネイル", "s"]
      ],
      "merge": {
        "poi": {
          "delete": true
        },
        "fid": {
          "delete": true
        }
      }
    },
    "refs": {
      "attrs": [
        ["fid", "ID", "n"],
        ["poi", "地物ID", "n"],
        ["book", "書籍ID", "n"],
        ["description", "説明", "s"],
        ["note", "ノート", "s"],
        ["anchor", "情報アンカ", "s"]
      ],
      "merge": {
        "book_": {
          "target": "books",
          "link": {
            "book": "fid"
          },
          "delete": true
        },
        "book": {
          "delete": true
        },
        "type": {
          "copy": "book_.type"
        },
        "title": {
          "copy": "book_.title"
        },
        "editor": {
          "copy": "book_.editor"
        },
        "publisher": {
          "copy": "book_.publisher"
        },
        "published_at": {
          "copy": "book_.published_at"
        },
        "url": {
          "copy": "book_.url"
        },
        "poi": {
          "delete": true
        },
        "fid": {
          "delete": true
        }
      }
    },
    "books": {
      "attrs": [
        ["fid", "ID", "n"],
        ["type", "種別", "s"],
        ["title", "書籍名", "s"],
        ["editor", "著者", "s"],
        ["publisher", "出版社", "s"],
        ["published_at", "出版年", "s"],
        ["url", "URL", "s"]
      ]
    }
  }
}