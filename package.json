{
  "name": "tatebayashistones",
  "version": "1.0.0",
  "description": "「たてばやしの野仏巡り」全5巻相当のデータのオープンデータ化＆現況調査プロジェクト",
  "main": "torii.js",
  "dependencies": {
    "argv": "0.0.2",
    "exif": "^0.6.0",
    "fs-extra": "^10.0.0",
    "jimp": "^0.16.1",
    "pkg": "^5.4.1",
    "xlsx": "^0.17.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_x2q_macm1": "pkg js/xlsx2Qgis.js --target=node16-darwin-arm64 --output=dist/xlsx2Qgis_macm1",
    "build_q2x_macm1": "pkg js/qgis2Xlsx.js --target=node16-darwin-arm64 --output=dist/qgis2Xlsx_macm1",
    "build_macm1": "npm run build_x2q_macm1 && npm run build_q2x_macm1 && npm run copy",
    "build_x2q_mac": "pkg js/xlsx2Qgis.js --target=node16-darwin-x64 --output=dist/xlsx2Qgis_mac",
    "build_q2x_mac": "pkg js/qgis2Xlsx.js --target=node16-darwin-x64 --output=dist/qgis2Xlsx_mac",
    "build_mac": "npm run build_x2q_mac && npm run build_q2x_mac && npm run copy",
    "build_x2q_win": "pkg js/xlsx2Qgis.js --target=node16-win-x64 --output=dist/xlsx2Qgis_win.exe",
    "build_q2x_win": "pkg js/qgis2Xlsx.js --target=node16-win-x64 --output=dist/qgis2Xlsx_win.exe",
    "build_win": "npm run build_x2q_win && npm run build_q2x_win && npm run copy",
    "copy": "cpx js/torii.json dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code4history/TatebayashiStones.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/code4history/TatebayashiStones/issues"
  },
  "homepage": "https://github.com/code4history/TatebayashiStones#readme",
  "pkg": {
    "assets": "./torii.json"
  },
  "devDependencies": {
    "cpx": "^1.5.0"
  }
}
